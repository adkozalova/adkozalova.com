{
  "name": "adkozalova.com",
  "homepage": "https://adkozalova.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "flag-icon-css": "^3.4.6",
    "i18next": "^19.6.3",
    "i18next-browser-languagedetector": "^6.0.0",
    "react": "^16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.0",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-slick": "^0.26.1",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy-s3": "aws s3 sync build/ s3://adkozalova.com --profile adkozalova.com --exclude \"*.DS_Store\"",
    "clear-cache": "aws cloudfront create-invalidation --distribution-id=E38A6T3RG6KI1N --path \"/*\" --profile adkozalova.com",
    "deploy": "npm run predeploy && npm run deploy-s3 && npm run clear-cache"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.2.0"
  }
}
